# Application Settings
# 애플리케이션 실행 환경 (development/staging/production)
APP_ENV=development
# 디버그 모드 활성화 여부
DEBUG=True
# 로깅 레벨 설정 (DEBUG/INFO/WARNING/ERROR/CRITICAL)
LOG_LEVEL=INFO

# Database Settings
# PostgreSQL 데이터베이스 호스트
DB_HOST=localhost
# PostgreSQL 데이터베이스 포트
DB_PORT=5432
# PostgreSQL 데이터베이스 사용자 이름
DB_USER=postgres
# PostgreSQL 데이터베이스 비밀번호
DB_PASSWORD=postgres
# PostgreSQL 데이터베이스 이름
DB_NAME=quizapp

# JWT Settings
# JWT 토큰 생성에 사용되는 비밀키 (반드시 변경하여 사용)
JWT_SECRET=your-super-secret-key-change-this-in-production
# JWT 토큰 암호화 알고리즘
JWT_ALGORITHM=HS256
# JWT 액세스 토큰 만료 시간 (분)
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Redis Settings
# Redis 서버 호스트
REDIS_HOST=localhost
# Redis 서버 포트
REDIS_PORT=6379
# Redis 데이터베이스 번호
REDIS_DB=0
# Redis 비밀번호 (필요한 경우)
REDIS_PASSWORD=
